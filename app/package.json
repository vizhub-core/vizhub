{
  "name": "app",
  "version": "1.0.0",
  "description": "Full stack app for VizHub3, based in Vite SSR.",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "tsc": "tsc src/entry-client.tsx --noEmit --allowJs",
    "prettier": "prettier {*.*,**/*.*} --single-quote --write",
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizhub-core/vizhub3.git"
  },
  "author": "Curran Kelleher",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vizhub-core/vizhub3/issues"
  },
  "homepage": "https://github.com/vizhub-core/vizhub3#readme",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.9",
    "@replit/codemirror-interact": "^6.1.0",
    "@sentry/node": "^7.54.0",
    "@teamwork/websocket-json-stream": "^2.0.0",
    "buble-jsx-only": "^0.21.0",
    "codemirror": "^6.0.1",
    "d3-transition": "^3.0.1",
    "express-openid-connect": "^2.16.0",
    "jose": "^4.14.4",
    "jsdom": "^22.1.0",
    "jsesc": "^3.0.2",
    "marked": "^5.1.0",
    "mongodb-legacy": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.0",
    "rollup-pluginutils": "^2.8.2",
    "sharedb": "^4.0.0",
    "sharedb-mongo": "^4.0.0",
    "vizhub-libraries": "^1.8.0",
    "vizhub-ui": "^3.1.0",
    "ws": "^8.13.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.63.0",
    "serve-static": "^1.15.0",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  }
}
