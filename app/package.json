{
  "name": "app",
  "version": "1.0.0",
  "description": "Full stack app for VizHub3, based in Vite SSR.",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "tsc": "tsc --noEmit",
    "prettier": "prettier {*.*,**/*.*} --single-quote --write",
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizhub-core/vizhub3.git"
  },
  "author": "Curran Kelleher",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vizhub-core/vizhub3/issues"
  },
  "homepage": "https://github.com/vizhub-core/vizhub3#readme",
  "dependencies": {
    "@rollup/browser": "^3.29.4",
    "@sentry/node": "^7.73.0",
    "@teamwork/websocket-json-stream": "^2.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/sharedb": "^3.3.3",
    "buble": "^0.20.0",
    "buble-jsx-only": "^0.21.0",
    "cookie": "^0.5.0",
    "d3-transition": "^3.0.1",
    "express-openid-connect": "^2.17.0",
    "jose": "^4.15.2",
    "jsdom": "^22.1.0",
    "jsesc": "^3.0.2",
    "jszip": "^3.10.1",
    "marked": "^9.1.0",
    "marked-gfm-heading-id": "^3.1.0",
    "mongodb-legacy": "^6.0.0",
    "path": "^0.12.7",
    "path-posix": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "rollup-pluginutils": "^2.8.2",
    "sharedb": "^4.1.0",
    "sharedb-mongo": "^4.1.0",
    "stripe": "^13.9.0",
    "vizhub-libraries": "^1.8.0",
    "vizhub-ui": "^3.4.0",
    "vzcode": "^0.23.0",
    "ws": "^8.14.2",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.1.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.2",
    "serve-static": "^1.15.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  }
}
