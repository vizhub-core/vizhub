  "bugs": {
    "url": "https://github.com/vizhub-core/vizhub3/issues"
  },
  "homepage": "https://github.com/vizhub-core/vizhub3#readme",
  "dependencies": {
    "jsesc": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "prettier": "^2.8.4",
    "serve-static": "^1.15.0",
    "vite": "^4.1.1"
  }
