{
  "name": "app",
  "version": "1.0.0",
  "description": "Full stack app for VizHub3, based in Vite SSR.",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "tsc": "tsc --noEmit",
    "prettier": "prettier {*.*,**/*.*} --single-quote --write",
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "//build:client": "NODE_ENV=development vite build --mode development --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production pm2 start server.js --name 'app-server' --no-daemon",
    "///serve": "NODE_ENV=production node server",
    "//serve": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizhub-core/vizhub3.git"
  },
  "author": "Curran Kelleher",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vizhub-core/vizhub3/issues"
  },
  "homepage": "https://github.com/vizhub-core/vizhub3#readme",
  "dependencies": {
    "@langchain/core": "^0.3.40",
    "@langchain/openai": "^0.4.4",
    "@rollup/browser": "^4.28.0",
    "@sentry/node": "^8.42.0",
    "@teamwork/websocket-json-stream": "^2.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/sharedb": "^5.1.0",
    "buble": "^0.20.0",
    "buble-jsx-only": "^0.21.0",
    "cookie": "^1.0.2",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-selection": "^3.0.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "express-openid-connect": "^2.17.1",
    "http-proxy-middleware": "^3.0.3",
    "jose": "^5.9.6",
    "jsdom": "^25.0.1",
    "llm-code-format": "^0.1.2",
    "marked": "^15.0.3",
    "mongodb-legacy": "^6.1.3",
    "partysocket": "^1.0.2",
    "path": "^0.12.7",
