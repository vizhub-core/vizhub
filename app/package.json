{
  "name": "app",
  "version": "1.0.0",
  "description": "Full stack app for VizHub3, based in Vite SSR.",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "tsc": "tsc --noEmit",
    "prettier": "prettier {*.*,**/*.*} --single-quote --write",
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "//build:client": "NODE_ENV=development vite build --mode development --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production pm2 start server.js --name 'app-server' --no-daemon",
    "///serve": "NODE_ENV=production node server",
    "//serve": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizhub-core/vizhub3.git"
  },
  "author": "Curran Kelleher",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vizhub-core/vizhub3/issues"
  },
  "homepage": "https://github.com/vizhub-core/vizhub3#readme",
  "dependencies": {
    "@rollup/browser": "^4.12.1",
    "@sentry/node": "^7.105.0",
    "@teamwork/websocket-json-stream": "^2.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/sharedb": "^3.3.10",
    "buble": "^0.20.0",
    "buble-jsx-only": "^0.21.0",
    "cookie": "^0.6.0",
    "d3-format": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "express-openid-connect": "^2.17.1",
    "http-proxy-middleware": "^2.0.6",
    "jose": "^5.2.2",
    "jsdom": "^24.0.0",
    "marked": "^12.0.1",
    "mongodb-legacy": "^6.0.1",
    "partysocket": "^1.0.1",
    "path": "^0.12.7",
    "path-posix": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.2",
    "remark-gfm": "^4.0.0",
    "rollup": "^4.12.1",
    "rollup-pluginutils": "^2.8.2",
    "sharedb": "^4.1.2",
    "sharedb-mongo": "^4.1.1",
    "stripe": "^14.19.0",
    "vizhub-libraries": "^1.8.0",
    "vizhub-ui": "^3.21.0",
    "vzcode": "^0.72.0",
    "ws": "^8.16.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/d3-format": "^3.0.4",
    "@types/d3-time-format": "^4.0.3",
    "@types/jsdom": "^21.1.6",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "compression": "^1.7.4",
    "express": "^4.18.3",
    "pm2": "^5.3.1",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "serve-static": "^1.15.0",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
  }
}
